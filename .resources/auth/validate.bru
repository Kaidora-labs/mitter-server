meta {
  name: Validate
  type: http
  seq: 3
}

post {
  url: {{BASE_URL}}/auth/validate
  body: json
  auth: inherit
}

body:json {
  {
    "emailAddress": "{{EMAIL_ADDRESS}}",
    "otp": "{{OTP}}"
  }
}

vars:pre-request {
  OTP: {{process.env.OTP}}
}

settings {
  encodeUrl: true
  timeout: 0
}
