    "phoneNumber": "{{PHONE_NUMBER}}",
    "emailAddress": "{{EMAIL_ADDRESS}}",
    "walletAddress": "{{WALLET_ADDRESS}}",
    "password": "{{PASSWORD}}",
    "role": "INDIVIDUAL"
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
