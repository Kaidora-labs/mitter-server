meta {
  name: Validate Auth
  type: http
  seq: 2
}

post {
  url: {{BASE_URL}}/auth/validate
  body: json
  auth: inherit
}

body:json {
  {
    "email_address": "{{EMAIL_ADDRESS}}",
    "password": "{{PASSWORD}}",
    "otp": "{{OTP}}"
  }
}

vars:pre-request {
  OTP: 625357
}

settings {
  encodeUrl: true
  timeout: 0
}
